{
  "name": "data-gov-agent",
  "version": "1.0.0",
  "description": "An agentic microservice for answering NL questions about US government datasets",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "test": "vitest",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "agentic",
    "microservice",
    "api",
    "us government",
    "datasets"
  ],
  "author": "Shai Mann <shai-mann>",
  "dependencies": {
    "@duckdb/node-api": "^1.4.0-r.1",
    "@langchain/core": "^0.3.77",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/openai": "^0.6.13",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "exceljs": "^4.4.0",
    "hono": "^4.0.0",
    "langchain": "1.0.0-alpha.6",
    "nock": "^14.0.10"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jest-image-snapshot": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest-image-snapshot": "^6.5.1",
    "prettier": "^3.2.5",
    "sharp": "^0.34.4",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
